<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net9.0;net8.0</TargetFrameworks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <IsAotCompatible>True</IsAotCompatible>
    <IsTrimmable>True</IsTrimmable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Palmtree.Core" Version="1.20.1-alpha" />
    <PackageReference Include="Palmtree.IO" Version="1.19.4-alpha" />
    <PackageReference Include="Palmtree.IO.Console" Version="1.7.4-alpha" />
    <PackageReference Include="Palmtree.Numerics" Version="1.2.15-alpha" />
  </ItemGroup>

</Project>
